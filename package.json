{
  "name": "vue-koa-boilerplate",
  "version": "1.0.0",
  "description": "Vue & Koa isomorphic boilerplate",
  "author": "luventa <luventa@outlook.com>",
  "scripts": {
    "analyze": "cross-env NODE_ENV=production MODE=analyze node scripts/build.js",
    "clean": "rimraf dist",
    "clean:dll": "rimraf src/dll",
    "dll": "npm run clean:dll && webpack --config ./scripts/webpack.dll.conf.js",
    "build": "cross-env NODE_ENV=testing node scripts/build.js",
    "build:client": "cross-env NODE_ENV=testing TASK=client node scripts/build.js",
    "build:server": "cross-env NODE_ENV=testing TASK=server node scripts/build.js",
    "dist": "cross-env NODE_ENV=production node scripts/build.js",
    "dist:client": "cross-env NODE_ENV=production TASK=client node scripts/build.js",
    "dist:server": "cross-env NODE_ENV=production TASK=server node scripts/build.js",
    "start": "cross-env NODE_ENV=development node scripts/dev-server.js",
    "package": "node scripts/package && npm run clean"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-decorators": "^0.20.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "archiver": "^2.1.1",
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
