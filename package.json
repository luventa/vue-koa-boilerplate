    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.3"
  },
  "engines": {
    "node": ">= 7.6.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ]
